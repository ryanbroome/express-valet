{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "name",
        "regionId",
        "address",
        "city",
        "state",
        "zipCode",
        "phone"
    ],
    "properties": {
        "id": {
            "type": "integer",
            "default": 0,
            "title": "The id Schema",
            "examples": [
                1
            ]
        },
        "name": {
            "type": "string",
            "default": "Unknown",
            "title": "The name Schema",
            "examples": [
                "MCH-MAB"
            ]
        },
        "regionId": {
            "type": "integer",
            "default": 1,
            "title": "The regionId Schema",
            "examples": [
                1
            ]
        },
        "address": {
            "type": "string",
            "default": "",
            "title": "The address Schema",
            "examples": [
                "123 Main St"
            ]
        },
        "city": {
            "type": "string",
            "default": "",
            "title": "The city Schema",
            "examples": [
                "Tampa"
            ]
        },
        "state": {
            "type": "string",
            "default": "",
            "title": "The state Schema",
            "examples": [
                "FL"
            ]
        },
        "zipCode": {
            "type": "string",
            "default": "",
            "title": "The zipCode Schema",
            "examples": [
                "33601"
            ]
        },
        "phone": {
            "type": "string",
            "default": "",
            "title": "The phone Schema",
            "examples": [
                "(123) 456-7890"
            ]
        }
    },
    "examples": [{
        "id": 1,
        "name": "MCH-MAB",
        "regionId": 1,
        "address": "123 Main St",
        "city": "Tampa",
        "state": "FL",
        "zipCode": "33601",
        "phone": "(123) 456-7890"
    }]
}