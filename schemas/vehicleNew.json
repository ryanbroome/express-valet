{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "ticketNum",
        "statusId",
        "mobile",
        "color",
        "make",
        "damages"
    ],
    "properties": {
        "ticketNum": {
            "type": "integer",
            "default": 0,
            "title": "The ticketNum Schema",
            "examples": [
                1001
            ]
        },
        "statusId": {
            "type": "integer",
            "default": 0,
            "title": "The statusId Schema",
            "examples": [
                1
            ]
        },
        "mobile": {
            "type": "string",
            "default": "",
            "title": "The mobile Schema",
            "examples": [
                "555-123-4567"
            ]
        },
        "color": {
            "type": "string",
            "default": "",
            "title": "The color Schema",
            "examples": [
                "blue"
            ]
        },
        "make": {
            "type": "string",
            "default": "",
            "title": "The make Schema",
            "examples": [
                "honda"
            ]
        },
        "damages": {
            "type": "string",
            "default": "",
            "title": "The damages Schema",
            "examples": [
                "hood, bumper, rims scratched"
            ]
        },
        "notes" :{
            "type": "string",
            "default": "",
            "title": "The notes Schema",
            "examples": [
                "notes"
            ]
        }
    },
    "examples": [{
        "ticketNum": 1001,
        "statusId": 1,
        "mobile": "555-123-4567",
        "color": "blue",
        "make": "honda",
        "damages": "[{\"x\":222.71875,\"y\":143}]",
        "notes": "notes"
    }]
}