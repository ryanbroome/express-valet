{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "title": "Root Schema",
    "type": "object",
    "default": {},
    "required": [
        "username",
        "password",
        "firstName",
        "lastName",
        "email",
        "phone",
        "roleId"
    ],
    "properties": {
        "id": {
            "title": "The id Schema",
            "type": "integer",
            "default": 0,
            "examples": [
                1
            ]
        },
        "username": {
            "title": "The username Schema",
            "type": "string",
            "default": "",
            "examples": [
                "username"
            ]
        },
        "password": {
            "title": "The password Schema",
            "type": "string",
            "default": "",
            "examples": [
                "password"
            ]
        },
        "firstName": {
            "title": "The firstName Schema",
            "type": "string",
            "default": "",
            "examples": [
                "firstName"
            ]
        },
        "lastName": {
            "title": "The lastName Schema",
            "type": "string",
            "default": "",
            "examples": [
                "lastName"
            ]
        },
        "email": {
            "title": "The email Schema",
            "type": "string",
            "default": "",
            "examples": [
                "username@gmail.com"
            ]
        },
        "phone": {
            "title": "The phone Schema",
            "type": "string",
            "default": "",
            "examples": [
                "555-123-4567"
            ]
        },
        "roleId": {
            "title": "The roleId Schema",
            "type": "integer",
            "default": 0,
            "examples": [
                1
            ]
        },
        "podiumId": {
            "title": "The podiumId  Schema",
            "type": "integer",
            "default": 0,
            "examples": [
                1
            ]
        }
    },
    "examples": [{
        "id": 1,
        "username": "username",
        "password": "password",
        "firstName": "firstName",
        "lastName": "lastName",
        "email": "username@gmail.com",
        "phone": "555-123-4567",
        "roleId": 1,
        "podiumId": 1
    }]
}