{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "title": "Root Schema",
    "type": "object",
    "default": {},
    "required": [
        "id",
        "username",
        "password",
        "first_name",
        "last_name",
        "email",
        "phone",
        "is_admin"
    ],
    "properties": {
        "id": {
            "title": "The id Schema",
            "type": "integer",
            "default": 0,
            "examples": [
                1
            ]
        },
        "username": {
            "title": "The username Schema",
            "type": "string",
            "default": "",
            "examples": [
                "username"
            ]
        },
        "password": {
            "title": "The password Schema",
            "type": "string",
            "default": "",
            "examples": [
                "password"
            ]
        },
        "first_name": {
            "title": "The first_name Schema",
            "type": "string",
            "default": "",
            "examples": [
                "first_name"
            ]
        },
        "last_name": {
            "title": "The last_name Schema",
            "type": "string",
            "default": "",
            "examples": [
                "last_name"
            ]
        },
        "email": {
            "title": "The email Schema",
            "type": "string",
            "default": "",
            "examples": [
                "username@gmail.com"
            ]
        },
        "phone": {
            "title": "The phone Schema",
            "type": "string",
            "default": "",
            "examples": [
                "555-123-4567"
            ]
        },
        "is_admin": {
            "title": "The is_admin Schema",
            "type": "boolean",
            "default": false,
            "examples": [
                false
            ]
        }
    },
    "examples": [{
        "id": 1,
        "username": "username",
        "password": "password",
        "first_name": "first_name",
        "last_name": "last_name",
        "email": "username@gmail.com",
        "phone": "555-123-4567",
        "is_admin": false
    }]
}