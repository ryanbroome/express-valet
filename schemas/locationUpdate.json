{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "properties": {
        "id": {
            "type": "integer",
            "default": 0,
            "title": "The id Schema",
            "examples": [
                1
            ]
        },
        "name": {
            "type": "string",
            "default": "Unknown",
            "title": "The name Schema",
            "examples": [
                "MCH-MAB"
            ]
        },
        "region_id": {
            "type": "integer",
            "default": 1,
            "title": "The region_id Schema",
            "examples": [
                1
            ]
        },
        "address": {
            "type": "string",
            "default": "",
            "title": "The address Schema",
            "examples": [
                "123 Main St"
            ]
        },
        "city": {
            "type": "string",
            "default": "",
            "title": "The city Schema",
            "examples": [
                "Tampa"
            ]
        },
        "state": {
            "type": "string",
            "default": "",
            "title": "The state Schema",
            "examples": [
                "FL"
            ]
        },
        "zip_code": {
            "type": "string",
            "default": "",
            "title": "The zip_code Schema",
            "examples": [
                "33601"
            ]
        },
        "phone": {
            "type": "string",
            "default": "",
            "title": "The phone Schema",
            "examples": [
                "(123) 456-7890"
            ]
        }
    },
    "examples": [{
        "id": 1,
        "name": "MCH-MAB",
        "region_id": 1,
        "address": "123 Main St",
        "city": "Tampa",
        "state": "FL",
        "zip_code": "33601",
        "phone": "(123) 456-7890"
    }]
}