{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "username",
        "password",
        "firstName",
        "lastName",
        "email",
        "phone"
    ],
    "properties": {
        "username": {
            "type": "string",
            "default": "",
            "title": "The username Schema",
            "examples": [
                "username"
            ]
        },
        "password": {
            "type": "string",
            "default": "",
            "title": "The password Schema",
            "examples": [
                "password"
            ]
        },
        "firstName": {
            "type": "string",
            "default": "",
            "title": "The firstName Schema",
            "examples": [
                "first"
            ]
        },
        "lastName": {
            "type": "string",
            "default": "",
            "title": "The lastName Schema",
            "examples": [
                "last"
            ]
        },
        "email": {
            "type": "string",
            "default": "",
            "title": "The email Schema",
            "examples": [
                "user@gmail.com"
            ]
        },
        "phone": {
            "type": "string",
            "default": "",
            "title": "The phone Schema",
            "examples": [
                "5551234567"
            ]
        },
        "isAdmin": {
            "type": "boolean",
            "default": false,
            "title": "The isAdmin Schema",
            "examples": [
                true
            ]
        }
    },
    "examples": [{
        "username": "username",
        "password": "password",
        "firstName": "first",
        "lastName": "last",
        "email": "user@gmail.com",
        "phone": "5551234567",
        "isAdmin": true
    }]
}